class Example95

{

static int a=m1();

static

{

System.out.println("Example95 SB");

}

int x=m2();

{

System.out.println("Example95 NSB");

}

Example95()

{

System.out.println("Example95 Constructor");

}

static int m1()

{

System.out.println("Example95 static variable is created");

return 10;

}

int m2()

{

System.out.println("Example95 non-static variable is created");

return 20;

}

void abc()

{

System.out.println("Example95 abc");

}

void bbc()

{

System.out.println("Example95 bbc");

}

}

class Sample95 extends Example95

{

static int b=m3();

static

{

System.out.println("Sample95 SB");

}

int y=m4();

{

System.out.println("Sample95 NSB");

}

Sample95()

{

System.out.println("Sample95 Constructor");

}

static int m3()

{

System.out.println("Sample95 static variable is created");

return 30;

}

int m4()

{

System.out.println("Sample95 non-static variable is created");

return 40;

}

void abc()

{

System.out.println("Sample95 abc");

}

public static void main(String args[])

{

System.out.println("SAMPLE95 main");

Sample95 s=new Sample95();

s.abc();

s.bbc();

}

}
